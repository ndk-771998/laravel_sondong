{
    "name": "vicoders/product_management",
    "description": "Boilerplate template for laravel package",
    "authors": [
        {
            "name": "sonhaichu",
            "email": "sonhaichu@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [],
        "psr-4": {
            "VCComponent\\Laravel\\Product\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VCComponent\\Laravel\\Product\\Test\\": "tests/"
        }
    },
    "require": {
        "vicoders/core": "@dev",
        "cviebrock/eloquent-sluggable": "6.x-dev",
        "maatwebsite/excel": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^4.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "VCComponent\\Laravel\\Product\\Providers\\ProductServiceProvider",
                "VCComponent\\Laravel\\Product\\Providers\\ProductRouteProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --color=always"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage-html coverages"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "../vc_laravel_module_core"
        }
    ]
}
