{
    "name": "vicoders/comment",
    "description": "",
    "authors": [
        {
            "name": "sonhaichu",
            "email": "sonhaichu@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [],
        "psr-4": {
            "VCComponent\\Laravel\\Comment\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VCComponent\\Laravel\\Comment\\Test\\": "tests/"
        }
    },
    "require": {
        "vicoders/core": "@dev",
        "cviebrock/eloquent-sluggable": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^4.3",
        "spatie/phpunit-watcher": "^1.21"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "VCComponent\\Laravel\\Comment\\Providers\\CommentServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --color=always"
        ],
        "test:watch": [
            "vendor/bin/phpunit-watcher watch"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage-html coverages"
        ]
    }
}
