{
    "name": "vicoders/postmanager",
    "authors": [
        {
            "name": "sonhaichu",
            "email": "sonhaichu@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [],
        "psr-4": {
            "VCComponent\\Laravel\\Post\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VCComponent\\Laravel\\Post\\Test\\": "tests/"
        }
    },
    "require": {
        "vicoders/core": "@dev",
        "cviebrock/eloquent-sluggable": "^7.0.1",
        "vicoders/view_model": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^4.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": [
            "vendor/bin/phpunit --color=always"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage-html coverages"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "../vc_laravel_module_core"
        },
        {
            "type": "path",
            "url": "../vc_laravel_view_model"
        }
    ]
}
