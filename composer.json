{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "cviebrock/eloquent-sluggable": "^7.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.0",
        "laravel/socialite": "4.4.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "2.1",
        "nesbot/carbon": "2.32.0",
        "webpress/analytics": "^0.0.4",
        "webpress/category-manager": "^0.0.4",
        "webpress/comment-manager": "^0.0.3",
        "webpress/configuration": "^0.0.5",
        "webpress/contact-manager": "^0.0.6",
        "webpress/core": "^0.0.1",
        "webpress/dynamic": "^1.0",
        "webpress/media-manager": "0.0.5",
        "webpress/menu-manager": "^0.0.7",
        "webpress/order-manager": "^0.0.6",
        "webpress/payment": "^0.0.3",
        "webpress/post-manager": "^0.0.6",
        "webpress/product-manager": "0.0.7",
        "webpress/script-manager": "^0.0.4",
        "webpress/search": "^0.0.3",
        "webpress/seo": "^0.0.3",
        "webpress/sitemap": "^0.0.3",
        "webpress/tag-manager": "^0.0.3",
        "webpress/user-manager": "^0.0.4",
        "webpress/utility": "^0.0.6",
        "webpress/view-model": "^0.0.5"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
